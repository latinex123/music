#Inicializar repositorio

$ git init

#Ner nuevos cambios
$git status

#Agregar el area de preparacion
$ git add index.html

#Agregar todos los archivos
$ git add -A

#Definir  nodo temporal
#Guardar cambios con un mensaje
$ git commit -m "Iniciar proyecto"

#Listar commits
$ git log

#Mostrar ayuda
$ git help

#Ver ramas
$ git branch

#Crear rama
$ git branc nombreRama

#Moverse a la rama
$ git checkout nombreRama

#Eliminar Rama
$ git branch -D nombreRama

#
$ git merge nombreRama 

#salvar los comiits
$ git log > commits.txt

#Moverse al commit
$ git checkout SHA-CODE

#Hacer la vinculacion
$ git remote add origin URL

#Comprobar la conexion
$ git remote -v

#Eliminar el vinculo
$ git remote remove origin

#Mandar nuevos commits a GitHub
$ git push origin rama
$ git push original master

#Clonar un repo existente
$ git clone URL

#Ver ramas ocultas
$ git branch -a

#Bajar cambios de master a la rama oculta
$ git fetch origin master

#Absorber los cambios de la rama oculta en master
$ git merge origin/master



